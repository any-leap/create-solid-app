{
  "name": "@any-l/create-solid-app",
  "version": "3.1.0",
  "description": "智能化的 TanStack Solid Start 项目脚手架",
  "type": "module",
  "bin": {
    "create-solid-app": "./bin/create-app.js"
  },
  "files": [
    "bin",
    "templates",
    "lib",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "publish:npm": "npm publish",
    "publish:github": "npm publish --registry=https://npm.pkg.github.com",
    "check:templates": "node scripts/template-consistency-checker.js",
    "sync:templates": "node scripts/sync-template-versions.js",
    "fix:templates": "node scripts/fix-templates.js",
    "test:templates": "node scripts/test-templates.js",
    "templates:all": "npm run fix:templates && npm run sync:templates && npm run check:templates && npm run test:templates",
    "templates": "node scripts/template-manager.js"
  },
  "keywords": [
    "solid",
    "tanstack",
    "scaffold",
    "cli",
    "template",
    "typescript"
  ],
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "enquirer": "^2.4.1",
    "execa": "^9.5.1",
    "fs-extra": "^11.3.0",
    "ora": "^8.1.1",
    "prompts": "^2.4.2",
    "rimraf": "^6.0.1",
    "semver": "^7.6.3",
    "validate-npm-package-name": "^6.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.5.4",
    "@types/prompts": "^2.4.9",
    "@types/semver": "^7.5.8",
    "@types/validate-npm-package-name": "^4.0.2",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/any-leap/create-solid-app.git"
  },
  "author": "Your Name <your.email@example.com>",
