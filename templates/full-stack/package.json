{
  "name": "my-solid-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && tsc --noEmit",
    "start": "vite start",
    "preview": "vite preview",
    "smart-start": "bun scripts/smart-start.js",
    "kill-port": "lsof -ti:3000 | xargs kill -9 2>/dev/null || echo '端口 3000 已清理'",
    "reset": "bun run kill-port && bun run build && bun run start",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:migrate": "drizzle-kit push:sqlite",
    "db:studio": "drizzle-kit studio",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f app",
    "docker:shell": "docker-compose exec app /bin/sh"
  },
  "dependencies": {
    "@kobalte/core": "^0.13.11",
    "@modular-forms/solid": "^0.25.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@tanstack/solid-devtools": "^0.1.1",
    "@tanstack/solid-query": "^5.83.1",
    "@tanstack/solid-router": "^1.130.12",
    "@tanstack/solid-router-devtools": "^1.130.13",
    "@tanstack/solid-start": "^1.130.15",
    "lucide-solid": "^0.536.0",
    "redaxios": "^0.5.1",
    "solid-js": "^1.9.5",
    "tailwind-merge": "^2.6.0",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^22.5.4",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.7.2",
    "vite": "^7.0.6",
    "vite-plugin-solid": "^2.11.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}